# Comment here!
let fizzbuzz' = 
    (a b) => if a <= b {
        if a % 3 == 0 && a % 5 == 0 {
            print "fizzbuzz"
        } elsif a % 3 == 0 {
            print "fizz"
        } elsif a % 5 == 0 {
            print "buzz"
        } else {
            print b
        }
        fizzbuzz' (a - 1) b
    } else {
        null
    }

# Curried functions
let fizzbuzz = fizzbuzz' 0

fizzbuzz 15

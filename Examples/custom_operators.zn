let (+) = (a b) => add a b
opdef (+) 6
let (-) = (a b) => sub a b
opdef (-) 6

let (*) = (a b) => mul a b
opdef (*) 5
let (/) = (a b) => div a b
opdef (/) 5
let (%) = (a b) => mod a b
opdef (%) 5

let (==) = (a b) => eq a b
opdef (==) 10

let @(-) = (a) => neg a
opdef @(-) 3

let (!)@ = (a) => fac a
opdef (!)@ 3
